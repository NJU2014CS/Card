<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.njucs.card"
    android:versionCode="1"
    android:versionName="1.0" >
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_CONTACTS"/><!-- 读取通讯录权限 -->  
	<uses-permission android:name="android.permission.WRITE_CONTACTS"/><!-- 写入通讯录权限 -->
	<uses-permission android:name="android.permission.INTERNET"/>

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
       </activity>
       
       <activity android:name=".user.User"></activity>
       
       <activity android:name=".contact.ContactActivity"></activity>
       
       <activity android:name=".tools.web.WebTest"></activity>
       
       <activity android:name=".connection.TestConnect"></activity>
       
       <activity android:name=".record.CheckRecordActivity"></activity>

       
    </application>

</manifest>
